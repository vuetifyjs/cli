    "is-wsl": "^3.1.0",
    "jsonc-parser": "^3.3.1",
    "kolorist": "catalog:",
    "nypm": "catalog:",
    "pathe": "catalog:",
    "pkg-types": "catalog:",
    "std-env": "^3.9.0",
    "tinyexec": "catalog:",
    "which": "catalog:"
