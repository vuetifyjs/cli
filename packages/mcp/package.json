{
  "name": "@vuetify/mcp",
  "version": "0.5.0",
  "description": "Model Context Protocol CLI for Vuetify assistance",
  "bin": {
    "vuetify-mcp-cli": "./dist/index.mjs"
  },
  "main": "./dist/index.mjs",
  "type": "module",
