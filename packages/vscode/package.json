{
  "publisher": "vuetify",
  "name": "vuetify-vscode",
  "displayName": "Vuetify",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.cjs",
  "engines": {
    "vscode": "^1.99.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "vuetify.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "vuetify.createProject",
        "title": "Create Vuetify Project",
        "category": "Vuetify"
      }
    ],
    "configuration": {
      "title": "Vuetify",
      "properties": {
        "vuetify.message": {
          "type": "string",
          "default": "Hello World",
          "description": "The message to show in the notification"
        }
      }
    }
  },
  "scripts": {
    "build": "tsdown --env.NODE_ENV production",
    "dev": "tsdown --watch ./src --env.NODE_ENV development",
    "typecheck": "tsc --noEmit",
    "vscode:prepublish": "pnpm run build"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "@types/vscode": "^1.99.0",
    "@vuetify/cli-shared": "workspace:*",
    "nypm": "catalog:",
    "pathe": "catalog:",
    "reactive-vscode": "^0.4.1",
    "tsdown": "catalog:",
    "typescript": "catalog:"
  }
}
